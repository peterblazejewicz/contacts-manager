{
    "name": "contacts-manager",
    "version": "1.0.0",
    "description": "Contacts Manager app by Todd Motto with Webpack, TS support, BS4, angular-translate to support your migration",
    "scripts": {
        "build": "npm-run-all build:*",
        "build:ts": "tsc",
        "clean:vendor": "rimraf src/vendor",
        "clean": "npm-run-all clean:*",
        "copy:vendor": "vendor-copy",
        "copy": "npm-run-all copy:*",
        "postinstall": "npm-run-all clean copy",
        "start": "serve --single src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/peterblazejewicz/contacts-manager.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/peterblazejewicz/contacts-manager/issues"
    },
    "homepage": "https://github.com/peterblazejewicz/contacts-manager#readme",
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-free-brands": "^5.0.6",
        "@fortawesome/fontawesome-free-regular": "^5.0.6",
        "@fortawesome/fontawesome-free-solid": "^5.0.6",
        "@uirouter/angularjs": "^1.0.3",
        "angular": "1.5.9",
        "angular-loading-bar": "^0.9.0",
        "angular-messages": "1.5.9",
        "angular-translate": "^2.15.2",
        "angularfire": "^2.3.0",
        "bootstrap": "^4.0.0",
        "firebase": "^4.10.1",
        "font-awesome": "^4.7.0"
    },
    "devDependencies": {
        "@types/angular": "^1.5.9",
        "@types/angular-loading-bar": "0.0.32",
        "@types/angular-mocks": "^1.5.10",
        "@types/angular-translate": "^2.15.1",
        "@types/angular-ui-router": "^1.1.37",
        "@types/angularfire": "^0.8.33",
        "@types/core-js": "^0.9.41",
        "@types/jasmine": "^2.5.47",
        "@types/node": "^7.0.14",
        "@types/rimraf": "^2.0.2",
        "@types/webpack-env": "^1.13.0",
        "angular-mocks": "^1.6.4",
        "core-js": "^2.4.1",
        "coveralls": "^2.13.1",
        "http-server": "^0.10.0",
        "jasmine-core": "^2.7.0",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-jasmine": "^1.1.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "ng-annotate-loader": "^0.6.1",
        "node-sass": "^4.5.3",
        "npm-run-all": "^4.1.2",
        "remap-istanbul": "^0.9.5",
        "rimraf": "^2.6.2",
        "sass-lint": "^1.11.1",
        "serve": "^6.4.11",
        "tslint": "^5.5.0",
        "typescript": "^2.7.2",
        "vendor-copy": "^1.3.1"
    },
    "vendorCopy": [
        {
            "from": "node_modules/angular/angular.js",
            "to": "src/vendor/angular/angular.js"
        },
        {
            "from": "node_modules/@uirouter/angularjs/release/angular-ui-router.js",
            "to": "src/vendor/angular/angular-ui-router.js"
        },
        {
            "from": "node_modules/bootstrap/dist/css/bootstrap.css",
            "to": "src/vendor/bootstrap/bootstrap.css"
        },
        {
            "from": "node_modules/@fortawesome/fontawesome/index.js",
            "to": "src/vendor/fontawesome/index.js"
        },
        {
            "from": "node_modules/@fortawesome/fontawesome-free-brands/index.js",
            "to": "src/vendor/fontawesome/free-brands.js"
        },
        {
            "from": "node_modules/@fortawesome/fontawesome-free-regular/index.js",
            "to": "src/vendor/fontawesome/free-regular.js"
        },
        {
            "from": "node_modules/@fortawesome/fontawesome-free-solid/index.js",
            "to": "src/vendor/fontawesome/free-solid.js"
        },
        {
            "from": "node_modules/firebase/firebase.js",
            "to": "src/vendor/firebase/firebase.js"
        },
        {
            "from": "node_modules/angularfire/dist/angularfire.js",
            "to": "src/vendor/angularfire/angularfire.js"
        },
        {
            "from": "node_modules/angular-messages/angular-messages.js",
            "to": "src/vendor/angular-messages/angular-messages.js"
        },
        {
            "from": "node_modules/angular-translate/dist/angular-translate.js",
            "to": "src/vendor/angular-translate/angular-translate.js"
        },
        {
            "from": "node_modules/angular-loading-bar/build/loading-bar.js",
            "to": "src/vendor/angular-loading-bar/loading-bar.js"
        },
        {
            "from": "node_modules/angular-loading-bar/build/loading-bar.css",
            "to": "src/vendor/angular-loading-bar/loading-bar.css"
        }
    ]
}
